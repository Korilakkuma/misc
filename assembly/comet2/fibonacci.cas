MAIN	START
		LAD	GR1,10
		CALL	FIB
		RET

FIB		PUSH	0,GR2
		CPA	GR1,=0
		JNZ	NOT0
		LAD	GR2,0
		JUMP	TERM

NOT0	CPA	GR1,=1
		JNZ	ABOVE1
		LAD	GR2,1
		JUMP	TERM

ABOVE1	PUSH	0,GR1
		LAD	GR1,-1,GR1	; n - 1
		CALL	FIB
		LD	GR2,GR1
		POP	GR1
		LAD	GR1,-2,GR1	; n - 2
		CALL	FIB
		ADDA	GR2,GR1	; F[n] = F[n - 1] + F[n - 2]

TERM	LD	GR1,GR2
		POP	GR2
		RET

		END
